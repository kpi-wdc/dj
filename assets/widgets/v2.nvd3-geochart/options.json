{
    "chart": {
        "type": "mapChart",
        "height": 450,
        "isArea": true,
        "margin": {
            "top": 20,
            "right": 10,
            "bottom": 40,
            "left": 0
        },

        "showLabels": false,
        "showValues": false,
        "showTiles": true,
        "selectedTiles": "mapbox.outdoors",
        "interactive": true,
        "defaultFill": "#f0f0f0",
        "defaultFillOpacity": 0,
        "defaultStroke": "#909095",
        "defaultStrokeWidth": 1,
        "defaultStrokeOpacity": 0.75,
        "selectedFillOpacity": 0.5,
        "selectedStrokeWidth": 3,
        "locale":"en",
        "boundary":{},



        "useInteractiveGuideline": false,
        "dispatch": {},
        "lines": {
            "grid": true,
            "axisLabel": true,
            "tickLabel": true,
            "ticks": 5,
            "clipVoronoi": true,
            "useVoronoi": true,
            "interactive": true
        },
        "legend": {
            "dispatch": {},
            "backgroundOpacity" : 0.5,
            "margin": {
                "top": 5,
                "right": 0,
                "bottom": 5,
                "left": 0
            },
            "align": true,
            "rightAlign": true,
            "updateState": true,
            "radioButtonMode": true
        },
        "interactiveLayer": {
            "dispatch": {},
            "margin": {
                "left": 55,
                "top": 30
            },
            "width": null,
            "height": null,
            "showGuideLine": true,
            "svgContainer": null
        },
        "clipVoronoi": true,
        "useVoronoi": true,
        "interactive": true,
        "showLegend": true,
        "tooltips": true,
        "state": {
            "disabled": [
                true,
                true,
                false
            ]
        },
        "defaultState": null,
        "noData": "No Data Available.",
        "transitionDuration": 250
    },
    "title": {
        "enable": true,
        "text": "Write Your Title",
        "class": "chartTitle",
        "css": {
            "width": "nullpx",
            "textAlign": "center"
        }
    },
    "subtitle": {
        "enable": true,
        "text": "Write Your Subtitle",
        "class": "chartSubTitle"
    },
    "caption": {
        "enable": true,
        "text": "Figure 1. Write Your Caption text.",
        "class": "chartCaption"
    },
    "styles": {
        "classes": {
            "with-3d-shadow": true,
            "with-transitions": true,
            "gallery": false
        },
        "css": {}
    }
